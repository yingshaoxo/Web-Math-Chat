js","original":{"line":76,"column":4}},{"name":"box_key_pressing","generated":{"line":83,"column":26},"source":"index.js","original":{"line":76,"column":26}},{"name":"bind","generated":{"line":83,"column":43},"source":"index.js","original":{"line":76,"column":43}},{"generated":{"line":83,"column":47},"source":"index.js","original":{"line":76,"column":26}},{"generated":{"line":83,"column":49},"source":"index.js","original":{"line":76,"column":4}},{"generated":{"line":84,"column":0},"source":"index.js","original":{"line":78,"column":1}},{"generated":{"line":84,"column":1},"source":"index.js","original":{"line":5,"column":0}}],"sources":{"index.js":"//jquery need to be load after document been loaded\n//$ = require(\"jquery\")\nwindow.$ = require(\"jquery\")\n\n$(document).ready( () => {\n\n    class chat_control {\n        constructor() {\n            this.msg_list = $('.msg-group');\n        }\n\n        send_msg(name, msg) {\n            this.msg_list.append(this.get_msg_html(name, msg, 'right'));\n            this.scroll_to_bottom(); \n        }\n\n        receive_msg(name, msg) {\n            this.msg_list.append(this.get_msg_html(name, msg, 'left'));\n            this.scroll_to_bottom(); \n        }\n\n        get_msg_html(name, msg, side) {\n            var msg_temple = `\n                <div class=\"card\">\n                     <div class=\"card-body\">\n                         <h6 class=\"card-subtitle mb-2 text-muted text-${side}\">${name}</h6>\n                         <p class=\"card-text float-${side}\">${msg}</p>\n                     </div>\n                </div>\n                `;\n            return msg_temple;\n        }\n\n        scroll_to_bottom() {\n            this.msg_list.scrollTop(this.msg_list[0].scrollHeight);\n        }\n    }\n\n\n    var chat = new chat_control();\n    chat.receive_msg('yingshaoxo', 'This was made for you! \\`2017/(520*1314)\\`');\n\n    send_button = $('button') // get jquery element from html table name\n    input_box = $('#input-box') // get jquery element from div id\n    // also you could get it by $('.form-control') or $('textarea')\n\n    function handle_msg(msg) {\n        msg = msg.trim()\n        msg = msg.replace(/(?:\\r\\n|\\r|\\n)/g, '<br>')\n        return msg\n    }\n\n    function send_msg() {\n        msg = handle_msg(input_box.val());\n        if (msg != '') {\n            chat.send_msg('you', msg);\n            input_box.val('');\n\n            setTimeout(function(){ chat.receive_msg('yingshaoxo', 'I can hear you.'); }, 1000);\n            MathJax.Hub.Queue([\"Typeset\",MathJax.Hub]);\n        }\n    }\n\n    function box_key_pressing(event) {\n        // control + enter was pressed\n        if ((event.keyCode === 10 || event.keyCode === 13) && event.ctrlKey) {\n            send_msg();\n        }\n        // esc was pressed\n        if (event.keyCode === 27) {\n            input_box.blur();\n        }\n    }\n\n    send_button.on('click', send_msg.bind());\n    input_box.on('keyup', box_key_pressing.bind());\n\n})\n"},"lineCount":null}},"hash":"35e004cfad298d25a4a52204b2e2d064","cacheData":{"env":{}}}